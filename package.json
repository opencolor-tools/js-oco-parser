{
  "name": "oco-parser",
  "version": "1.0.0",
  "description": "Parser for the open color format",
  "main": "lib/index.js",
  "repository": "https://github.com/opencolor-tools/js-oco-parser",
  "watch": {
    "test": {
      "patterns": [
        "grammar",
        "lib",
        "test"
      ],
      "extensions": "js,jison"
    }
  },
  "scripts": {
    "test": "make test",
    "watch": "npm-watch",
    "lint": "jshint lib/ test/"
  },
  "engines": {
    "node": "> 4.0"
  },
  "keywords": [
    "colors",
    "palettes",
    "parsing",
    "yaml"
  ],
  "author": "Jan Krutisch",
  "license": "MIT",
  "devDependencies": {
    "js-yaml": "^3.5.4",
    "jison": "^0.4.17",
    "lex": "^1.7.9",
    "chai": "^3.5.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "npm-watch": "^0.1.1"
  }
}
